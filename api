url: /*
method: get
    responses:
        200 - { status: 404, message:"not found"}

url: /tokens
method: get
    responses:
        200 - Will return array of available tokens
        404 - if something went wrong

url: /practices?page=1
method: get
    headers:
        x-access-token: token required
    queries:
        page: number what page with practices to show
            *if not specified will show all practices
            *shows 5 practices per page
    responses:
        200 - Will return array of available practices
        401 - if there is no token provided in headers
        404 - if something went wrong

url: /practices/:practiseId/technologies?page=1
method: get
    headers:
        x-access-token: token required
    parameters:
        practiceId: id of the practice
    queries:
        page: number what page with practices to show
            *if not specified will show all practices
            *shows 5 practices per page
    responses:
        200 - Will return array of available practises
        401 - if there is no token provided in headers
        404 - if something went wrong
